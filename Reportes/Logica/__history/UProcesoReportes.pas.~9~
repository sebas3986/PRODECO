implementation

var
  ListArch          : TSearchRec;
  FrmAplicacionCtrl : TFrmAplicacionCtrl;


Function TProcesoReportes.VerificarVersion(p_NombModu: string;p_VersModu: string): boolean;
begin
  FrmAplicacionCtrl:= TFrmAplicacionCtrl.create(FrmAplicacionCtrl);
  //FrmAplicacionCtrl.
  //if FrmAplicacionCtrl.Version <> p_VersModu then
   // FrmAplicacionCtrl.ShowModal;
  //FrmAplicacionCtrl.free;
  {
  FDatosAplicacion.ConfigurarAplicacion(p_NombModu);
  if FDatosAplicacion.Version <> p_VersModu then
    Result:= False
  else
    Result:= True;
  }
end;

{$REGION 'CONSTRUTOR AND DESTRUCTOR'}
constructor TProcesoReportes.Create;
begin
{
  try
    FDatosAplicacion    := TAplicacion.create;
  except
    on e:Exception do
      raise Exception.Create('No es posible Inicializar Componentes. '
                              + #10#13 + '* ' + e.Message);
  end;
  }
end;

destructor TProcesoReportes.Destroy;
begin
  //FDatosAplicacion.Free;
end;
{$ENDREGION}

